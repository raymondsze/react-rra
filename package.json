{
  "name": "react-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "precommit": "npm run lint && npm run lint:css && npm run test",
    "prepush": "npm run lint && npm run lint:css && npm run test",
    "start": "node ./build/server/main.js",
    "develop": "nodemon ./build/server/main.js | ./node_modules/.bin/bunyan",
    "build-client": "babel-node node_modules/.bin/webpack --config ./webpack/webpack.config-client.js",
    "build-server": "babel-node node_modules/.bin/webpack --config ./webpack/webpack.config-server.js",
    "build-client-dev": "babel-node ./webpack/webpack.server.js",
    "build-client-dev-no-watch": "babel-node node_modules/.bin/webpack --config ./webpack/webpack.config-client.js --env.dev",
    "build-server-dev": "babel-node node_modules/.bin/webpack --watch --config ./webpack/webpack.config-server.js --env.dev",
    "build-server-dev-no-watch": "babel-node node_modules/.bin/webpack --config ./webpack/webpack.config-server.js --env.dev",
    "lint": "eslint src --ext .js --ext .jsx --ext .gql --ext .graphql",
    "lint:css": "stylelint \"./src/**/*.{js,jsx}\"",
    "translate": "babel-node ./scripts/translate.js",
    "schema": "babel-node ./scripts/schema.js",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "bundle-analyze": "babel-node node_modules/.bin/webpack --config ./webpack/webpack.config-client.js --profile --json > .stats.json && node_modules/.bin/webpack-bundle-analyzer .stat.json build/client"
  },
